{
  "name": "arca_rest_api",
  "version": "0.0.1",
  "description": "Web Server Rest API that retrieve data from ARCA",
  "main": "server.js",
  "bin": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "dev": "nodemon ts-node --inspect=5858 src/server.ts",
    "start": "node dist/server.js",
    "prod": "npm run build && npm run start",
    "pkg": "npm run build && pkg . --debug --out-path build/"
  },
  "private": true,
  "author": "Luca Ciotti",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.18.3",
    "config": "^2.0.2",
    "dateformat": "^3.0.3",
    "excel4node": "^1.7.0",
    "express": "^4.16.3",
    "json-query": "^2.2.2",
    "lodash": "^4.17.13",
    "morgan": "^1.9.0",
    "node-adodb": "^4.2.2",
    "request": "^2.87.0",
    "rotating-file-stream": "^1.3.9",
    "typescript-collections": "^1.3.2",
    "uuid": "^3.4.0"
  },
  "devDependencies": {
    "@types/config": "0.0.34",
    "@types/express": "^4.16.0",
    "@types/morgan": "^1.7.35",
    "@types/request": "^2.48.1",
    "nodemon": "^1.18.7"
  },
  "pkg": {
    "scripts": [
      "dist/appConst.js"
    ],
    "assets": [
      "config/default.json",
      "node_modules/config/**/*.*",
      "node_modules/log4js/**/*.*"
    ],
    "targets": [
      "node4-win-x64",
      "node4-win-x86"
    ]
  }
}
